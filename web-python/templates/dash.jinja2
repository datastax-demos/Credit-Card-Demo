{% extends "base.jinja2" %}

{% block title %}Ticker{% endblock %}

{% block body %}
<div class="dash-body">
<div id="page-generic">


    <!-- separate NAV for DashPage START -->
    <div id="top-navigation" class="clearfix"><a href="/portfolio"><img
